{
  "printWidth":80,
  "tabWidth":2,
  "useTabs":true,
  "semi":true,
  "singleQuote":true,
  "trailingComma":"all",
  "bracketSpacing":true,
  "proseWrap":"always",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
